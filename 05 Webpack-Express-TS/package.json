{
  "name": "05_webpack5_express",
  "version": "1.0.0",
  "description": "Custom Webpack 5, Express.js and TS config",
  "main": "./src/server.ts",
  "scripts": {
    "server": "clear && NODE_ENV=development nodemon ./src/server.ts",
    "client": "BROWSER=NONE npm start --prefix ../client/",
    "dev": "clear && concurrently \"npm run server\"  \"npm run client\"",
    "build": "clear && NODE_ENV=production webpack --mode production --config webpack.config.ts",
    "done": "clear && node dist/bundleApi.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
